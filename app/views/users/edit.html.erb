<h1>Edit Profile</h1>


<%= form_for @user do |form| %>
        
<div class="text-field">
  <%= form.label :fullname %>
  <%= form.text_field :fullname %>
</div>

<div class="text-field">
  <%= form.label :username %>
  <%= form.text_field :username %>
</div>

<div class="text-field">
  <%= form.label :email %>
  <%= form.text_field :email, type: 'email' %>
</div>


<div class="submit-field">
<%= form.file_field :avatars, multiple: true%>
  <%= form.submit %>
</div>
<% end %>
